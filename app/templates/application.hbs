{{#each flashMessages.queue as |flash|}}
  {{#flash-message flash=flash as |component flash|}}
    {{flash.message}}
  {{/flash-message}}
{{/each}}

<div class="container">
  {{
    nav-bar
    session=session
    invalidateSession=(action 'invalidateSession')
    sessionRequiresAuthentication=(action 'sessionRequiresAuthentication')
  }}

  {{outlet}}
</div>
